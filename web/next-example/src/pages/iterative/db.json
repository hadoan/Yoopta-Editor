            "type": "bulleted-list",
            "children": [
              {
                "text": "Dynamic Document/Text Editor",
                "bold": true
              }
            ],
            "props": {
              "nodeType": "block"
            }
          }
        ],
        "type": "BulletedList",
        "meta": {
          "order": 5,
          "depth": 0,
          "align": "left"
        }
